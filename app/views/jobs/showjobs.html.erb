<div class="container">
	<div class="row">
		<div class="col-md-12">
		<table class="table table-hover table-responsive table-striped table-jobs">
			<thead>
				<tr class="thead-jobs">
					<th colspan="4" class="text-center"><%= @webscraped.upcase %></th>
				</tr>
			</thead>
			<% @jobs.each do |job| %>
			  <tr class="row-jobs">
			  	<td class="td-date-job"><%= job.date %></td>
			  	<td class="text-center"><%= job.company %></td>
			  	<td class="td-title-job"><%= job.title %></td>
			  	<td><%= job.country %></td>
			  </tr>
		  	<% end %>
		</table>
		</div>
	</div>
</div>